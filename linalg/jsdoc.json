{
    "plugins": ["plugins/markdown"],
    "sourceType": "module",
    "source": {
        "include": ["src/complex.js",
                    "src/matrix.js",
                    "src/polynomial.js",
                    "src/subspace.js",
                    "src/vector.js"]
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": false
    },
    "opts": {
        "destination": "./doc/",
        "template": "node_modules/foodoc/template",
        "readme": "README.md"
    },
    "templates": {
        "systemName": "linalg.js",
        "systemSummary": "Library for learning linear algebra",
        "copyright": "",
        "collapseSymbols": false,
        "stylesheets": ["../static/linalgdoc.css"],
        "scripts": ["../static/doc.js"],
        "systemColor": "#012169"
    }
}
